{
  "checkout-cart": {
    "blocks": [
      "checkout-cart-single-col",
      "checkout-cart-two-cols",
      "empty-state"
    ]
  },
  "empty-state": {
    "children": ["flex-layout.row#empty-state"]
  },
  "flex-layout.row#empty-state": {
    "children": ["flex-layout.col#empty-state"],
    "props": {
      "paddingTop": 10
    }
  },
  "flex-layout.col#empty-state": {
    "children": [
      "flex-layout.row#empty-state-icon",
      "flex-layout.row#empty-state-message"
    ]
  },
  "flex-layout.row#empty-state-icon": {
    "children": ["empty-state-icon"]
  },
  "flex-layout.row#empty-state-message": {
    "children": ["empty-state-message"]
  },  
  "checkout-cart-single-col": {
    "children": [
      "flex-layout.row#single-col"
    ]
  },
  "flex-layout.row#single-col": {
    "children": [
      "flex-layout.col#single"
    ],
    "props": {
      "marginBottom": "6"
    }
  },
  "flex-layout.col#single": {
    "children": [
      "flex-layout.row#title",
      "flex-layout.row#product-list",
      "flex-layout.row#shipping",
      "flex-layout.row#summary",
      "flex-layout.row#continue-shopping.block"
    ],
    "props": {
      "marginBottom": "6"
    }
  },
  "checkout-cart-two-cols": {
    "children": [
      "flex-layout.row#two-cols",
      "divider#horizontal"
    ]
  },
  "flex-layout.row#two-cols": {
    "children": [
      "flex-layout.col#left",
      "divider#vertical",
      "flex-layout.col#right"
    ],
    "props": {
      "colSizing": "auto",
      "marginTop": "2"
    }
  },
  "flex-layout.col#left": {
    "children": [
      "flex-layout.row#title",
      "flex-layout.row#product-list",
      "flex-layout.row#continue-shopping.inline"
    ],
    "props": {
      "width": "grow",
      "marginRight": "7",
      "preventVerticalStretch": "true"
    }
  },
  "flex-layout.row#title": {
    "children": ["title"]
  },
  "flex-layout.row#product-list": {
    "children": ["product-list-wrapper"],
    "props": {
      "marginBottom": "7"
    }
  },
  "product-list-wrapper": {
    "blocks": ["product-list"]
  },
  "flex-layout.row#continue-shopping.block": {
    "children": ["continue-shopping"],
    "props": {
      "marginTop": "7"
    }
  },
  "flex-layout.row#continue-shopping.inline": {
    "children": ["continue-shopping"],
    "props": {
      "marginBottom": "7",
      "horizontalAlign": "right",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.col#right": {
    "children": [
      "flex-layout.row#right"
    ]
  },
  "flex-layout.row#right": {
    "children": [
      "flex-layout.col#rightContent"
    ],
    "props": {
      "marginTop": "9",
      "marginBottom": "6"
    }
  },
  "flex-layout.col#rightContent": {
    "children": [
      "flex-layout.row#shipping",
      "flex-layout.row#summary"
    ],
    "props": {
      "blockClass": "sidebar",
      "marginLeft": "7",
      "preventVerticalStretch": "true",
      "width": "grow"
    }
  },
  "flex-layout.row#shipping": {
    "children": ["shipping-wrapper"],
    "props": {
      "marginBottom": "6"
    }
  },
  "shipping-wrapper": {
    "blocks": ["shipping-calculator"]
  },
  "flex-layout.row#summary": {
    "children": ["summary-wrapper"]
  },
  "summary-wrapper": {
    "blocks": ["checkout-summary"]
  },
  "divider#vertical": {
    "props": {
      "orientation": "vertical"
    }
  },
  "divider#horizontal": {
    "props": {
      "orientation": "horizontal"
    }
  }
}
